from os import *
from sys import *
from collections import *
from math import *

def printPascal(n:int):
    # Write your code here.
    # Return a list of lists.
    l=[]
    l.append([1])
    if(n==1):
        return l
    if(n==2):
        l.append([1,1])
        return l
    l.append([1,1])
    for i in range(3,n+1):
        temp=[0 for j in range(i)]
        temp[0]=1
        temp[-1]=1
        last=l[-1]
        for j in range(1,i-1):
            temp[j]=last[j-1]+last[j]
        l.append(temp)
    return l
